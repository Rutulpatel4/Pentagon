@model IEnumerable<Pentagon.Models.key>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<hr />
<hr />
<h2 align="center" style="font-family:'Century'; font-weight:bold; color:white; background-color:#282929">Keys</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.keyTrackingNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.vehicle)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.keyTrackingNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.vehicle)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.vehicleId }) |
                @Html.ActionLink("Details", "Details", new { id = item.vehicleId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.vehicleId })
            </td>
        </tr>
    }

</table>
